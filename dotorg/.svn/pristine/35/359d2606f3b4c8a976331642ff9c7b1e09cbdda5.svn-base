{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "distantjet/universalist-title",
  "version": "1.0.0",
  "title": "Universalist Title",
  "category": "widgets",
  "icon": "translation",
  "description": "Create titles for multiple languages with Type safety",
  "attributes": {
    "title_en": {
      "type": "string",
      "default": ""
    },
    "title_es": {
      "type": "string",
      "default": ""
    },
    "title_style": {
      "type": "string",
      "default": "h2"
    }
  },
  "textdomain": "universalist-title",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "render": "file:./render.php"
}